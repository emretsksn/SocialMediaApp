<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    app:startDestination="@id/homePage">
    <fragment
        android:id="@+id/homePage"
        android:name="com.emretaskesen.tpost.ui.fragment.main.HomePageFragment"
        android:label="fragment_home_page"
        tools:layout="@layout/fragment_home_page" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.emretaskesen.tpost.ui.fragment.main.SearchFragment"
        android:label="Search Fragment"
        tools:layout="@layout/fragment_search" />
    <fragment
        android:id="@+id/messagesFragment"
        android:name="com.emretaskesen.tpost.ui.fragment.main.MessagesFragment"
